    <jcr:content
        cq:isDelivered="{Boolean}false"
        cq:template="/libs/core/franklin/templates/page"
        jcr:primaryType="cq:PageContent"
        jcr:title="Vlad Băilescu"
        sling:resourceType="core/franklin/components/page/v1/page"
        affiliation="Adobe"
        jcr:mixinTypes="[cq:ReplicationStatus]"
        cq:lastReplicated="{Date}2024-04-30T00:00:00.000+02:00"
        cq:lastReplicationAction="Activate">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="core/franklin/components/root/v1/root">
            <section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/franklin/components/section/v1/section">
                <title
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Vlad Băilescu"
                    sling:resourceType="core/franklin/components/title/v1/title"
                    type="h1"/>
                <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/franklin/components/image/v1/image"
                    alt="Carsten Ziegeler"
                    fileReference="/content/dam/adaptto/xwalk/speaker/vlad-bailescu.jpg"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="core/franklin/components/text/v1/text"
                    text="&lt;p&gt;I’ve been working in software development for more than 15 years, dipping my toes into (almost) everything: from outsourcing for banking and telecom companies to building and operating a SaS GPS tracking solution, from small GUI utilities to high-availability large-scale transactional systems split across microservices.&lt;/p&gt;&#xA;&lt;p&gt;I’ve been with Adobe for almost 6 years, helping on improving HTL and creating building pieces for AEM sites – the Core Components. The thing that drove me to push forward through all this time was creating software that helps people.&lt;/p&gt;"/>
            </section>
        </root>
        <image jcr:primaryType="nt:unstructured"/>
    </jcr:content>
